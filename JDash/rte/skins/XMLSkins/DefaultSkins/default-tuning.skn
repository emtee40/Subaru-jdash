<skin name="JDash / Line Graphs for Tuning" extends="resources.inc">

<description>This simple skin will layout a series line graph gauges you might use for simple tuning.  It will show values like RPM, LOAD and STFT.  This skin will also display the Check Engine Light status, and as many as 8 Trouble Codes.  Assuming the selected monitor supportes DTC processing.</description>
  
  <!-- Main Window -->
  <window width="800" height="510" type="window" scale="20" fill-color="black">
	  
    <text color="green" size="12" x="10" y="500" format="Tuning Graphs" font="Arial"/>
    <text color="red" size="12" x="150" y="420" format="Reset"    font="Arial"/>
	  
    <!-- The Check Engine Light reset button -->
    <button 
          type="push-button" 
          x="200" 
          y="405" 
          width="20" 
          height="20" 
          up-image="red-push-button-up" 
          down-image="red-push-button-down">
      <event type="down" destination="monitor" action="dtc-reset"/>
    </button>
   
  </window>



  <!-- Event Triggers.  Respond to the DEFOG switch.  If it's turned on, then enable logging. -->
  <trigger sensor="DEFOG_SW" sensor-min="1" sensor-max="1">
    <event type="enter-range" destination="logger" action="enable"/>
  </trigger>

  
   <!-- column 1 -->
    <!-- RPM -->
	<gauge sensor="RPM" label="RPM" type="line-graph" x="10"  y="10"   width="280" height="90" seconds="60" sensor-min="0"   sensor-max="8000" format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
    </gauge>

	<!-- MPH -->
	<gauge sensor="MPH"           label="MPH"    type="line-graph" x="10"  y="110"  width="280" height="90" seconds="60" sensor-min="0"   sensor-max="150"  format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
    </gauge>
	
	<!-- LOAD -->
	<gauge sensor="LOAD"          label="Load %"   type="line-graph" x="10"  y="210"  width="280" height="90" seconds="60" sensor-min="0"   sensor-max="100"  format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
    </gauge>
	
	<!-- KNOCK CORRECTION -->
	<gauge sensor="KNOCK_COR_DEG" label="Knock Corr. deg" type="line-graph" x="10"  y="310"  width="280" height="90" seconds="60" sensor-min="-30" sensor-max="30"   format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
	 </gauge>
	
 <!-- Column 2 -->
	<!-- STFT -->
	<gauge sensor="STFT_1"      label="STFT %"     type="line-graph" x="400" y="10"   width="280" height="90" seconds="60" sensor-min="-30" sensor-max="30"  format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
	</gauge>
	
	<!-- LTFT -->
	<gauge sensor="LTFT_1"    label="LTFT %"     type="line-graph" x="400" y="110"  width="280" height="90" seconds="60" sensor-min="-30" sensor-max="30"  format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
	</gauge>
	
	<!-- TIMING -->
	<gauge sensor="IG_TIMING_DEG" label="Timing deg"   type="line-graph" x="400" y="210"  width="280" height="90" seconds="60" sensor-min="-30" sensor-max="60"  format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
    </gauge>

    <!-- LAMBDA -->
	<gauge sensor="LAMBDA_1" label="Lambda"  type="line-graph" x="400" y="310"  width="280" height="90" seconds="60" sensor-min="0"   sensor-max="2"  format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
    </gauge>
    
    <!-- INJECTOR PULSE -->
	<gauge sensor="INJ_1_PULSE_MS" label="Inj Pulse (ms)"  type="line-graph" x="400" y="410"  width="280" height="90" seconds="60" sensor-min="0"   sensor-max="20"  format="%f">
      <text type="high" color="red"   size="18" x="285" y="25"  format="%02.0f" font="digital"/>
      <text type="main" color="blue"  size="18" x="285" y="55"  format="%02.0f" font="digital"/>
      <text type="low"  color="green" size="18" x="285" y="85"  format="%02.0f" font="digital"/>
	</gauge>
    

  <!-- A button sensor connected to the defog switch -->
  <gauge sensor="DEFOG_SW" type="led" x="360" y="408">
    <static>
        <text color="green" size="12" x="-90" y="10" format="Defog-SW/Log" font="Arial"/>
     </static>
    <led sensor-min="0" sensor-max="0">
     	<rectangle color="dark-red" fill-color="none" line-width="1" x="0" y="-3" width="20" height="20" />
     </led>
     <led sensor-min="1" sensor-max="1">
     	<rectangle color="dark-red" fill-color="red" line-width="1" x="0" y="-3" width="20" height="20" />
     </led>
  </gauge>

  
    <!-- The MIL Check Engine Light -->
   <gauge sensor="MIL_STATUS" type="led" x="90" y="408">
     <static>
     	<text color="red" size="12" x="-80" y="10" format="Check Engine"    font="Arial"/>
     </static>
     <led sensor-min="0" sensor-max="0">
     	<round-rectangle color="dark-red" fill-color="none" line-width="2" x="0" y="0" width="16" height="16" arcw="4" arch="4"/>
     </led>
     <led sensor-min="1" sensor-max="1">
     	<round-rectangle color="red" fill-color="red" line-width="2" x="0" y="0" width="16" height="16" arcw="4" arch="4"/>
     </led>
   </gauge>


  <gauge sensor="DTC_0" type="digital" x="10" y="435" >
	  <text color="red" size="12" x="10" y="435" format="DTC-%s"  font="Arial"/>
  </gauge>
  
  <gauge sensor="DTC_1" type="digital" x="10" y="450">
	  <text color="red" size="12" x="0" y="0" format="DTC-%s"  font="Arial"/>
  </gauge>
  
  <gauge sensor="DTC_2" type="digital" x="10" y="465">
	  <text color="red" size="12" x="0" y="0" format="DTC-%s"  font="Arial"/>
  </gauge>
  
  <gauge sensor="DTC_3" type="digital" x="10" y="480">
	  <text color="red" size="12" x="0" y="0" format="DTC-%s"  font="Arial"/>
  </gauge>
  
  <gauge sensor="DTC_4" type="digital" x="150" y="435">
	  <text color="red" size="12" x="0" y="0" format="DTC-%s"  font="Arial"/>
  </gauge>
  
  <gauge sensor="DTC_5" type="digital" x="150" y="450">
	  <text color="red" size="12" x="0" y="0" format="DTC-%s"  font="Arial"/>
  </gauge>
  
  <gauge sensor="DTC_6" type="digital" x="150" y="465">
	  <text color="red" size="12" x="0" y="0" format="DTC-%s"  font="Arial"/>
  </gauge>
  
  <gauge sensor="DTC_7" type="digital" x="150" y="480">
	  <text color="red" size="12" x="0" y="0" format="DTC-%s"  font="Arial"/>
  </gauge>


  <!-- Digital Rate -->
  <gauge sensor="RATE" type="digital" x="120" y="500">
     <text color="blue" size="14" x="0" y="0" format="Refresh Rate (Hz): %02.2f" font="Arial"/>
  </gauge>


</skin>